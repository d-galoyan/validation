{
  "name": "v-for-validation",
  "version": "5.1.1",
  "description": "Simple validation written in typescript",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "link": "npm link",
    "unlink": "npm unlink",
    "build": "tsc -b tsconfig.json",
    "build:watch": "tsc -b tsconfig.json --watch",
    "clean": "rm -rf ./dist",
    "prepare": "npm run build",
    "lint": "eslint ./src --ext .ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.10.0",
    "jest": "^26.6.3",
    "pre-commit": "^1.2.2",
    "ts-jest": "^26.5.6",
    "typescript": "^4.4.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/d-galoyan/validation.git"
  },
